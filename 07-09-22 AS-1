#include<stdio.h>
int main()
{
int fib[25], i,n,a,c=0;
scanf("%d",&n);
fib[0] = 0;
fib[1] = 1;
for (i = 2; i < n; i++)
{
fib[i] = fib[i - 1] + fib[i- 2];
}
scanf("%d",&a);
for (i = 0; i < n; i++)
{
printf("%d ", fib[i]);
if(a==fib[i])
c=0;
else
c++;
}
if(c==0)
printf("YES");
else
printf("NO");
return 0;
}
