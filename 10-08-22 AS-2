#include <stdio.h>

int main()
{
    int n,sum=0,r,num;
    scanf("%d",&n);
    num=n;
    while(n>0)
    {
        
        r=n%10;
        sum=sum+r;
        n=n/10;
    }
    if(num/sum==0)
    printf("Harshad Number");
    else
    printf("Not Harshad Number");
    return 0;
}
OUTPUT:
 156
Harshad Number
22
Not Harshard Number
